{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"scopeName": "source.eet",
	"name": "EET",
	"patterns": [
        { "match": "^([0-9]+,)([ ]?[0-9]+,)([ ]?[0-9]+,?)(([^/][^/]*))?",
          "name": "eetgroup",
          "captures": {
              "1": {"name": "entity.name.type.timestamp.eet", "tooltip_name": "Stream Time"},
              "2": {"name": "constant.numeric.uat_index", "tooltip_name": "Unique Message Index"},
              "3": {"name": "keyword.instruction_id.eet", "tooltip_name": "Instruction ID"},
              "4": {"name": "variable.instruction.eet", "tooltip_name": "Data"}
          }
        }
    ],
	"repository": {
		"strings": {
			"name": "string.quoted.double.eet",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.eet",
					"match": "\\\\."
				}
			]
		}
	}
}