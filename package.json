{
    "name": "eet",
    "displayName": "EET",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:eet"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "eet",
                "aliases": [
                    "EET",
                    "eet"
                ],
                "extensions": [
                    ".eet"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "eet",
                "scopeName": "source.eet",
                "path": "./syntaxes/eet.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "lint": "eslint src --ext ts"
    },
    "capabilities": {
        "hoverProvider": "true"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/node": "^17.0.10",
        "@types/vscode": "^1.63.1",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.7.0",
        "glob": "^7.2.0",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.5",
        "vscode-test": "^1.6.1",
        "webpack": "^5.67.0",
        "webpack-cli": "^4.9.1"
    },
    "dependencies": {
        "vsce": "^2.6.3"
    }
}
